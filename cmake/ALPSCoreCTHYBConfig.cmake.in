# Package configuration file for ALPSCore CT-HYB solvers
include(FindPackageHandleStandardArgs)

set(ALPSCoreCTHYB_LIBRARIES "-L@CMAKE_INSTALL_PREFIX@/lib -lalps_cthyb_solver")
set(ALPSCoreCTHYB_INCLUDE_DIR "@CMAKE_INSTALL_PREFIX@/include")
set(ALPSCoreCTHYB_INCLUDE_DIRS ${ALPSCoreCTHYB_INCLUDE_DIR})

FIND_PACKAGE_HANDLE_STANDARD_ARGS(ALPSCoreCTHYB REQUIRED_VARS ALPSCoreCTHYB_INCLUDE_DIR ALPSCoreCTHYB_LIBRARIES FOUND_VAR ALPSCoreCTHYB_FOUND FAIL_MESSAGE DEFAULT_MSG)
